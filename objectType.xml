<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/maelstrom/objectType.xsd">
	<import>
		<!-- clipboard -->
		<type>
			<name>com.guilded.character</name>
			<definitionname>com.woltlab.wcf.clipboardItem</definitionname>
			<listclassname>gms\data\character\CharacterList</listclassname>
		</type>
		<!-- /clipboard -->

		<!-- search -->
		<type>
			<name>com.guilded.character</name>
			<definitionname>com.woltlab.wcf.searchableObjectType</definitionname>
			<classname>gms\system\search\CharacterSearch</classname>
		</type>
		<type>
			<name>com.guilded.guild</name>
			<definitionname>com.woltlab.wcf.searchableObjectType</definitionname>
			<classname>gms\system\search\GuildSearch</classname>
		</type>
		<!-- /search -->

		<!-- location -->
		<type>
			<name>com.guilded.CharacterPage</name>
			<definitionname>com.woltlab.wcf.user.online.location</definitionname>
			<controller>gms\page\CharacterPage</controller>
			<languagevariable>wcf.user.usersOnline.location.characterPage</languagevariable>
		</type>
		<type>
			<name>com.guilded.GuildPage</name>
			<definitionname>com.woltlab.wcf.user.online.location</definitionname>
			<controller>gms\page\GuildPage</controller>
			<languagevariable>wcf.user.usersOnline.location.guildPage</languagevariable>
		</type>
		<!-- /location -->

		<!-- dashboard -->
		<type>
			<name>com.guilded.gms.IndexPage</name>
			<definitionname>com.woltlab.wcf.user.dashboardContainer</definitionname>
			<allowcontent>1</allowcontent>
			<allowsidebar>1</allowsidebar>
		</type>
		<type>
			<name>com.guilded.gms.GuildPage</name>
			<definitionname>com.woltlab.wcf.user.dashboardContainer</definitionname>
			<allowcontent>1</allowcontent>
			<allowsidebar>1</allowsidebar>
		</type>
		<!-- /dashboard -->

		<!-- recent activity -->
		<type>
			<name>com.guilded.wcf.user.recentActivityEvent.character</name>
			<definitionname>com.woltlab.wcf.user.recentActivityEvent</definitionname>
			<classname>gms\system\user\activity\event\CharacterUserActivityEvent</classname>
		</type>
		<!-- /recent activity -->
	</import>
</data>
