<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/maelstrom/objectType.xsd">
	<import>
		<!-- clipboard -->
		<type>
			<name>com.guilded.gms.character</name>
			<definitionname>com.woltlab.wcf.clipboardItem</definitionname>
			<listclassname>gms\data\character\CharacterList</listclassname>
		</type>
		<type>
			<name>com.guilded.gms.guild</name>
			<definitionname>com.woltlab.wcf.clipboardItem</definitionname>
			<listclassname>gms\data\guild\GuildList</listclassname>
		</type>
		<type>
			<name>com.guilded.gms.event.date</name>
			<definitionname>com.woltlab.wcf.clipboardItem</definitionname>
			<listclassname>gms\data\event\date\EventDateList</listclassname>
		</type>
		<type>
			<name>com.guilded.gms.event.date.participation</name>
			<definitionname>com.woltlab.wcf.clipboardItem</definitionname>
			<listclassname>gms\data\event\date\participation\EventDateParticipationList</listclassname>
		</type>
		<!-- /clipboard -->

		<!-- search -->
		<type>
			<name>com.guilded.gms.character</name>
			<definitionname>com.woltlab.wcf.searchableObjectType</definitionname>
			<classname>gms\system\search\CharacterSearch</classname>
		</type>
		<type>
			<name>com.guilded.gms.guild</name>
			<definitionname>com.woltlab.wcf.searchableObjectType</definitionname>
			<classname>gms\system\search\GuildSearch</classname>
		</type>
		<!-- /search -->

		<!-- location -->
		<type>
			<name>com.guilded.gms.IndexPage</name>
			<definitionname>com.woltlab.wcf.user.online.location</definitionname>
			<controller>gms\page\IndexPage</controller>
			<languagevariable>gms.user.usersOnline.location.IndexPage</languagevariable>
		</type>
		<type>
			<name>com.guilded.gms.CalendarPage</name>
			<definitionname>com.woltlab.wcf.user.online.location</definitionname>
			<controller>gms\page\CalendarPage</controller>
			<languagevariable>gms.user.usersOnline.location.CalendarPage</languagevariable>
		</type>
		<type>
			<name>com.guilded.gms.EventPage</name>
			<definitionname>com.woltlab.wcf.user.online.location</definitionname>
			<controller>gms\page\EventPage</controller>
			<languagevariable>gms.user.usersOnline.location.EventPage</languagevariable>
		</type>
		<type>
			<name>com.guilded.gms.CharacterPage</name>
			<definitionname>com.woltlab.wcf.user.online.location</definitionname>
			<controller>gms\page\CharacterPage</controller>
			<languagevariable>gms.user.usersOnline.location.CharacterPage</languagevariable>
		</type>
		<type>
			<name>com.guilded.gms.GuildPage</name>
			<definitionname>com.woltlab.wcf.user.online.location</definitionname>
			<controller>gms\page\GuildPage</controller>
			<languagevariable>gms.user.usersOnline.location.GuildPage</languagevariable>
		</type>
		<!-- /location -->

		<!-- dashboard -->
		<type>
			<name>com.guilded.gms.IndexPage</name>
			<definitionname>com.woltlab.wcf.user.dashboardContainer</definitionname>
			<allowcontent>1</allowcontent>
			<allowsidebar>1</allowsidebar>
		</type>
		<type>
			<name>com.guilded.gms.CalendarPage</name>
			<definitionname>com.woltlab.wcf.user.dashboardContainer</definitionname>
			<allowcontent>0</allowcontent>
			<allowsidebar>1</allowsidebar>
		</type>
		<type>
			<name>com.guilded.gms.EventPage</name>
			<definitionname>com.woltlab.wcf.user.dashboardContainer</definitionname>
			<allowcontent>0</allowcontent>
			<allowsidebar>1</allowsidebar>
		</type>
		<type>
			<name>com.guilded.gms.CharacterPage</name>
			<definitionname>com.woltlab.wcf.user.dashboardContainer</definitionname>
			<allowcontent>0</allowcontent>
			<allowsidebar>1</allowsidebar>
		</type>
		<type>
			<name>com.guilded.gms.GuildPage</name>
			<definitionname>com.woltlab.wcf.user.dashboardContainer</definitionname>
			<allowcontent>0</allowcontent>
			<allowsidebar>1</allowsidebar>
		</type>
		<!-- /dashboard -->

		<!-- recent activity -->
		<type>
			<name>com.guilded.gms.user.recentActivityEvent.character</name> <!-- new character creation -->
			<definitionname>com.woltlab.wcf.user.recentActivityEvent</definitionname>
			<classname>gms\system\user\activity\event\CharacterUserActivityEvent</classname>
		</type>
		<type>
			<name>com.guilded.gms.user.recentActivityEvent.event.date.participation</name>
			<definitionname>com.woltlab.wcf.user.recentActivityEvent</definitionname>
			<classname>gms\system\user\activity\event\EventDateParticipationUserActivityEvent</classname>
		</type>
		<!-- /recent activity -->

		<!-- eventType -->
		<type>
			<name>com.guilded.gms.event.date</name>
			<definitionname>com.guilded.gms.event.type</definitionname>
			<classname>gms\system\event\type\DateEventType</classname>
		</type>
		<type>
			<name>com.guilded.gms.event.birthday</name>
			<definitionname>com.guilded.gms.event.type</definitionname>
			<classname>gms\system\event\type\BirthdayEventType</classname>
		</type>
		<type>
			<name>com.guilded.gms.event.raid</name>
			<definitionname>com.guilded.gms.event.type</definitionname>
			<classname>gms\system\event\type\RaidEventType</classname>
		</type>
		<!-- /eventType -->

		<!-- modification log -->
		<type>
			<name>com.guilded.gms.event</name>
			<definitionname>com.woltlab.wcf.modifiableContent</definitionname>
			<classname>gms\system\log\modification\EventModificationLogHandler</classname>
		</type>
		<type>
			<name>com.guilded.gms.event.participation</name>
			<definitionname>com.woltlab.wcf.modifiableContent</definitionname>
			<classname>gms\system\log\modification\EventDateParticipationModificationLogHandler</classname>
		</type>
		<!-- /modification log -->

		<!-- view -->
		<type>
			<name>com.guilded.gms.event.view.user</name>
			<definitionname>com.guilded.gms.event.view</definitionname>
			<classname>gms\system\event\view\UserEventView</classname>
		</type>
		<type>
			<name>com.guilded.gms.event.view.classification</name>
			<definitionname>com.guilded.gms.event.view</definitionname>
			<classname>gms\system\event\view\GameClassificationEventView</classname>
		</type>
		<type>
			<name>com.guilded.gms.event.view.role</name>
			<definitionname>com.guilded.gms.event.view</definitionname>
			<classname>gms\system\event\view\GameRoleEventView</classname>
		</type>
		<!-- /view -->

		<!-- clipboard -->
		<type>
			<name>com.guilded.gms.event</name>
			<definitionname>com.woltlab.wcf.clipboardItem</definitionname>
			<listclassname>gms\data\event\EventList</listclassname>
		</type>
		<!-- /clipboard -->

		<!-- comment -->
		<type>
			<name>com.guilded.gms.event.date.comment</name>
			<definitionname>com.woltlab.wcf.comment.commentableContent</definitionname>
			<classname>gms\system\comment\manager\EventDateCommentManager</classname>
		</type>
		<!-- /comment -->

		<!-- moderation -->
		<type>
			<name>com.guilded.gms.event.date.participation</name>
			<definitionname>com.woltlab.wcf.moderation.activation</definitionname>
			<classname>gms\system\moderation\queue\activation\EventDateParticipationModerationQueueActivationHandler</classname>
		</type>
		<!-- /moderation -->

		<!-- category -->
		<type>
			<name>com.guilded.gms.credit.category</name>
			<definitionname>com.woltlab.wcf.category</definitionname>
			<classname>gms\system\category\CreditCategoryType</classname>
		</type>
		<type>
			<name>com.guilded.gms.event.category</name>
			<definitionname>com.woltlab.wcf.category</definitionname>
			<classname>gms\system\category\EventCategoryType</classname>
		</type>
		<!-- /category -->

		<!-- acl -->
		<type>
			<name>com.guilded.gms.event.category</name>
			<definitionname>com.woltlab.wcf.acl</definitionname>
		</type>
		<!-- /acl -->

		<!-- com.guilded.gms.credit -->
		<type>
			<name>com.guilded.gms.event.date.credit</name>
			<definitionname>com.guilded.gms.credit</definitionname>
			<classname>gms\system\event\date\credit\EventDateCredit</classname>
		</type>
		<!-- /com.guilded.gms.credit -->

		<!-- modification log -->
		<type>
			<name>com.guilded.gms.event.date</name>
			<definitionname>com.woltlab.wcf.modifiableContent</definitionname>
		</type>
		<type>
			<name>com.guilded.gms.event.date.participation</name>
			<definitionname>com.woltlab.wcf.modifiableContent</definitionname>
		</type>
		<!-- /modification log -->

		<!-- @todo object watch for guild -->
		<!-- @todo object watch for character -->
		<!-- @todo object watch for event -->
	</import>
</data>
