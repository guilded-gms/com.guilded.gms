			<controller><![CDATA[wcf\acp\page\CharacterListPage]]></controller>
			<parent>wcf.acp.menu.link.management.character</parent>
			<permissions>admin.guilded.character.canViewList</permissions>
			<showorder>1</showorder>
		</acpmenuitem>
