		<acpmenuitem name="wcf.acp.menu.link.management.guild.general">
			<parent>wcf.acp.menu.link.management.guild</parent>
			<showorder>1</showorder>
		</acpmenuitem>

		<acpmenuitem name="wcf.acp.menu.link.management.guild.list">
